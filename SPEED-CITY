				g.Outside.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			end
		end
	end
    
    for _,v in pairs(game:GetService("Workspace").DarkMap.Spawners:GetChildren()) do 
		wait()
		--spawner
		for _,g in pairs(v:GetChildren()) do
			wait() 
			--OrbNumber
			if g.Outside then
				wait()
				if _G.cheat == false then return end 
				g.Outside.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			end
		end
	end
    
    for _,v in pairs(game:GetService("Workspace").BeachMap.Spawners:GetChildren()) do 
		wait()
